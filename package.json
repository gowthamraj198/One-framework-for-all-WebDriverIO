    "@wdio/spec-reporter": "^6.7.0",
    "@wdio/sync": "^6.7.0",
    "allure-commandline": "2.13.0",
    "chromedriver": "^86.0.0",
    "eslint": "^7.12.1",
    "ffmpeg": "0.0.4",
    "log4js": "6.3.0",
    "nodemon": "^2.0.6",
    "pngjs": "6.0.0",
    "source-map-support": "^0.5.19",
    "wdio-chromedriver-service": "^6.0.4"
